{
  "name": "opensphere-plugin-analyze",
  "version": "1.0.0",
  "description": "Analyze window and tools for Open Sphere",
  "main": "index.js",
  "keywords": [],
  "author": "William Wall",
  "license": "ISC",
  "directories": {
    "src": "src",
    "test": "test",
    "scss": [
      "../bits-internal/scss",
      "../mist/scss"
    ]
  },
  "build": {
    "type": "plugin",
    "config": [
      "../mist/config/analyze-layout.json"
    ],
    "index": "tools.js",
    "scss": "scss/tools.scss",
    "defineRoots": {
      "mist.ROOT": "../mist/",
      "bits.ROOT": "../bits-internal/"
    },
    "ignoreUncompiled": {
      "plugin.tools.TOOLS_PATH": true
    },
    "gcc": {
      "entry_point": [
        "goog:plugin.tools.ToolsPlugin",
        "goog:benum",
        "goog:imagery",
        "goog:metacarta",
        "goog:omar",
        "goog:voyager"
      ],
      "define": [
        "plugin.tools.TOOLS_PATH=''"
      ],
      "externs": [
        "../bits-internal/externs/bits-internal.externs.js",
        "../bits-internal/externs/goldenlayout.externs.js",
        "../bits-internal/externs/vega.externs.js",
        "../bits-internal/externs/zingchart.externs.js"
      ],
      "js": [
        "../bits-internal/src/**.js",
        "../mist/src/**.js"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js'"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.0",
    "eslint-config-opensphere": "^1.0.0",
    "eslint-plugin-google-camelcase": "^0.0.2",
    "eslint-plugin-opensphere": "^2.0.0"
  },
  "dependencies": {
    "golden-layout": "^1.5.9",
    "opensphere": "^1.0.0",
    "zingchart": "2.6.1"
  }
}
