{
  "name": "opensphere-plugin-analyze",
  "version": "1.0.0",
  "description": "Analyze window and tools for OpenSphere",
  "keywords": [],
  "author": "William Wall",
  "license": "ISC",
  "directories": {
    "src": "src",
    "test": "test",
    "externs": "externs",
    "scss": "scss",
    "views": "views"
  },
  "build": {
    "type": "plugin",
    "config": [
      "config/analyze-layout.json"
    ],
    "gcc-src": [
      "src/**.js"
    ],
    "index": "tools.js",
    "scss": "scss/tools.scss",
    "ignoreUncompiled": {
      "plugin.tools.TOOLS_PATH": true
    },
    "gcc": {
      "entry_point": [
        "goog:plugin.tools.ToolsPlugin"
      ],
      "define": [
        "plugin.tools.TOOLS_PATH=''"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js'"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "eslint": "^7.26.0",
    "eslint-config-opensphere": "^5.0.0",
    "husky": "^3.0.1"
  },
  "dependencies": {
    "golden-layout": "^1.5.9",
    "milstd": "^0.1.6",
    "milsymbol": "^2.0.0",
    "opensphere": "0.0.0-development",
    "vega": "=5.19.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
