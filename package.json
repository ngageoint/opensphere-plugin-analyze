{
  "name": "opensphere-plugin-analyze",
  "version": "1.0.0",
  "description": "Analyze window and tools for OpenSphere",
  "keywords": [],
  "author": "William Wall",
  "license": "ISC",
  "directories": {
    "src": "src",
    "test": "test",
    "scss": [
      "../mist/scss"
    ]
  },
  "build": {
    "type": "plugin",
    "config": [
      "../mist/config/analyze-layout.json"
    ],
    "index": "tools.js",
    "scss": "scss/tools.scss",
    "defineRoots": {
      "mist.ROOT": "../mist/"
    },
    "ignoreUncompiled": {
      "plugin.tools.TOOLS_PATH": true
    },
    "gcc": {
      "entry_point": [
        "goog:plugin.tools.ToolsPlugin",
        "goog:benum",
        "goog:imagery",
        "goog:metacarta",
        "goog:omar",
        "goog:voyager"
      ],
      "define": [
        "os.SETTINGS_DB_NAME='opensphere.settings'",
        "os.FILE_DB_VERSION=2",
        "os.FILE_STORE_NAME='files'",
        "os.SHARED_DB_NAME='opensphere.shared'",
        "os.SHARED_STORE_NAME='shared'",
        "plugin.tools.TOOLS_PATH=''"
      ],
      "externs": [
        "../mist/externs/mist.externs.js"
      ],
      "js": [
        "../mist/src/**.js"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js'"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@commitlint/travis-cli": "^9.0.1",
    "eslint": "^6.0.0",
    "eslint-config-opensphere": "^4.0.0",
    "husky": "^3.0.1"
  },
  "dependencies": {
    "bits-internal": "^0.0.1",
    "golden-layout": "^1.5.9",
    "opensphere": "0.0.0-development"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
