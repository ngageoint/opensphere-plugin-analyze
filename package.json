{
  "name": "opensphere-plugin-analyze",
  "version": "1.0.0",
  "description": "Analyze window and tools for Open Sphere",
  "main": "index.js",
  "keywords": [],
  "author": "William Wall",
  "license": "ISC",
  "directories": {
    "src": "src",
    "test": "test",
    "scss": ["../bits-internal/scss", "../mist/scss"],
    "views": "views"
  },
  "build": {
    "type": "plugin",
    "index": "tools.js",
    "scss": "scss/tools.scss",
    "defineRoots": {
      "mist.ROOT": "../mist/",
      "plugin.mist.ROOT": "../mist/",
      "bits.ROOT": "../bits-internal/"
    },
    "ignoreUncompiled": {
      "plugin.mist.TOOLS_PATH": true,
      "plugin.tools.TOOLS_PATH": true
    },
    "gcc": {
      "entry_point": [
        "goog:plugin.tools.ToolsPlugin",
        "goog:omar",
        "goog:tools"
      ],
      "define": [
        "plugin.mist.TOOLS_PATH=''",
        "plugin.tools.TOOLS_PATH=''"
      ],
      "externs": [
        "../bits-internal/externs/bits-internal.externs.js",
        "../bits-internal/externs/zingchart.externs.js"
      ],
      "js": [
        "../bits-internal/src/**.js",
        "../mist/src/mist/**.js",
        "../mist/src/tools/**.js",
        "../mist/src/plugin/chart/scatter/**.js",
        "../mist/src/plugin/mist/analyze*.js",
        "../mist/src/plugin/mist/defines.js",
        "../mist/src/plugin/file/kml/kmlpluginext.js",
        "../mist/src/plugin/featureaction/**.js",
        "../mist/src/plugin/omar/**.js"
      ]
    }
  },
  "devDependencies": {
    "angular-mocks": "~1.5.x",
    "bits-protractor": "^0.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-recommended-bump": "^0.3.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.5.0",
    "eslint-config-google": "^0.7.0",
    "eslint-config-opensphere": "^0.2.0",
    "eslint-plugin-google-camelcase": "^0.0.2",
    "google-closure-compiler": "^20161201.0.0",
    "husky": "^0.13.3",
    "jasmine-core": "^2.5.2",
    "js-dossier": "^0.10.0",
    "jsdoc": "^3.4.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^0.1.0",
    "karma-junit-reporter": "^1.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "=3.10.1",
    "opensphere-build-closure-helper": "^0.2.0",
    "opensphere-build-docs": "^0.1.0",
    "opensphere-build-index": "^0.3.0",
    "opensphere-build-resolver": "^0.3.0",
    "opensphere-state-schema": "^2.0.0",
    "replace": "^0.3.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "validate-commit-msg": "^2.8.2",
    "xmllint": "^0.1.1"
  },
  "dependencies": {
    "opensphere": "^0.5.0"
  }
}
